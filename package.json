{
  "name": "hello-world-react",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start": "npm run clean && npm run build-server && npm run build-client",
    "build-server": "webpack --colors --display-error-details --config configs/webpack.node.js",
    "build-client": "webpack --colors --display-error-details --config configs/webpack.web.js",
    "dev1": "cross-env BABEL_ENV=development NODE_ENV=development nodemon server/index.js --ignore dist --ignore common --ignore src",
    "build-server1": "npm run clean && cross-env NODE_ENV=development DEBUG=app:* webpack --colors --display-error-details --config ./configs/webpack.node.js",
    "clean": "rm -rf ./dist && rm -rf ./www",
    "test1": "npm run clean && cross-env NODE_ENV=development DEBUG=app:* webpack-dev-server --watch --colors --hot  --config configs/webpack.node.watch.js ",
    "watch-server": "webpack --watch --colors --display-error-details --config configs/webpack.node.watch.js",
    "runServer": "cross-env NODE_ENV=development DEBUG=app:* node --harmony dist/server.js",
    "dev-server": "nodemon --watch runner/ --watch src/server/ --exec \"cross-env NODE_ENV=development DEBUG=app:* node --harmony runner/startServer.babel.js\"",
    "dev-client": "npm-run-all --parallel watch-client watch-client-dev",
    "dev": "npm-run-all --parallel dev-server dev-client",
    "watch-client": "webpack --watch --colors --display-error-details --config configs/webpack.web.watch.js",
    "watch-client-dev": "webpack-dev-server --config configs/webpack.web.watch.js"
  },
  "dependencies": {
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-react-display-name": "^2.0.0",
    "babel-plugin-transform-react-display-name": "^6.23.0",
    "babel-polyfill": "^6.23.0",
    "css-loader": "^0.27.1",
    "debug-logger": "^0.4.1",
    "express-http-proxy": "^0.11.0",
    "file-loader": "^0.10.1",
    "history": "^4.6.0",
    "html-webpack-plugin": "^2.28.0",
    "http-proxy": "^1.16.2",
    "ip": "^1.1.5",
    "koa": "^2.1.0",
    "koa-mount": "^2.0.0",
    "koa-proxy": "^0.8.0",
    "koa-router": "^7.1.0",
    "koa-static": "^3.0.0",
    "koa-static-cache": "^4.0.0",
    "npm-run-all": "^4.0.2",
    "path": "^0.12.7",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-hot-loader": "next",
    "react-router": "^4.0.0",
    "react-router-redux": "next",
    "react-snapshot": "^1.0.4",
    "redbox-react": "^1.3.4",
    "redux": "^3.6.0",
    "rimraf": "^2.6.1",
    "sass-loader": "^6.0.3",
    "style-loader": "^0.13.2",
    "url-loader": "^0.5.8",
    "webpack": "^2.2.1",
    "webpack-dev-middleware": "1.6.1",
    "webpack-dev-server": "^2.2.0",
    "webpack-hot-middleware": "2.10.0",
    "webpack-node-externals": "^1.5.4"
  },
  "devDependencies": {
    "babel-core": "^6.23.1",
    "babel-loader": "^6.3.2",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.23.0",
    "babel-preset-stage-0": "^6.22.0",
    "babel-register": "^6.23.0",
    "cross-env": "^3.2.3",
    "express": "^4.15.2",
    "extract-text-webpack-plugin": "^2.1.0",
    "nodemon": "^1.11.0",
    "react-snapshot": "^1.0.4"
  }
}
